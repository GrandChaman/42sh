echo It Works;
echo "Going tmp";
/bin/pwd | rev | sed -e 's/^\///' | rev;
cd /tmp;
/bin/pwd | rev | sed -e 's/^\///' | rev;
echo "Going back";
cd -;
echo "Going back 1 dir";
cd ..;
/bin/pwd | rev | sed -e 's/^\///' | rev;
echo "Going back";
cd -;
/bin/pwd | rev | sed -e 's/^\///' | rev;
echo "Going home";
cd;
/bin/pwd | rev | sed -e 's/^\///' | rev;
echo "Going back";
cd -;
/bin/pwd | rev | sed -e 's/^\///' | rev;
